{
	"name": "audio-mixer",
	"version": "1.0.3",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:full": "vite build && pnpm build-content && pnpm build-background",
		"build-content": "esbuild src/content.ts --bundle --minify --outdir=build/scripts",
		"build-background": "esbuild src/background.ts --bundle --minify --outdir=build/scripts",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.26.0",
		"@types/chrome": "^0.0.248",
		"@unocss/extractor-svelte": "^0.56.5",
		"@unocss/reset": "^0.56.5",
		"@unocss/transformer-directives": "^0.56.5",
		"esbuild": "^0.19.5",
		"svelte": "^4.0.0",
		"svelte-check": "^3.5.2",
		"sveltekit-adapter-chrome-extension": "^2.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"unocss": "^0.56.5",
		"vite": "^4.0.0"
	},
	"type": "module"
}
